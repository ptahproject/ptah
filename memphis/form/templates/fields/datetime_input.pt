<tal:block define="dummy view.include('jquery-ui')"/>
<script>
  $(function() {
     $("#${context.id}-date" ).datepicker();
  });
</script>

<input id="" name="" class="" title="" lang="" disabled=""
       readonly="" alt="" tabindex="" accesskey="" size="" maxlength=""
       value="" type="text"
       tal:attributes="id '%s-date'%context.id;
                       name context.date_name;
                       class 'small %s'%context.klass;
                       title context.title;
                       lang context.lang;
                       value context.date_part;
                       disabled context.disabled;
                       tabindex context.tabindex;
                       readonly context.readonly;
                       alt context.alt;
                       accesskey context.accesskey;
                       size context.size;
                       maxlength context.maxlength" />

